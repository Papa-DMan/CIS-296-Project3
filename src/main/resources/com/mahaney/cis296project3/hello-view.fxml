<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.mahaney.cis296project3.HelloController">
    <Label fx:id="stackDisplay" text="" layoutX="25" layoutY="50"/>
    <GridPane alignment="CENTER" prefHeight="400" prefWidth="200" hgap="0" vgap="0">
        <Button fx:id="0" text="0" GridPane.columnIndex="1" GridPane.rowIndex="4" onAction="#calcInputCallback" styleClass="numbers"/>
        <Button fx:id="1" text="1" GridPane.columnIndex="0" GridPane.rowIndex="3" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="2" text="2" GridPane.columnIndex="1" GridPane.rowIndex="3" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="3" text="3" GridPane.columnIndex="2" GridPane.rowIndex="3" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="4" text="4" GridPane.columnIndex="0" GridPane.rowIndex="2" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="5" text="5" GridPane.columnIndex="1" GridPane.rowIndex="2" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="6" text="6" GridPane.columnIndex="2" GridPane.rowIndex="2" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="7" text="7" GridPane.columnIndex="0" GridPane.rowIndex="1" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="8" text="8" GridPane.columnIndex="1" GridPane.rowIndex="1" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="9" text="9" GridPane.columnIndex="2" GridPane.rowIndex="1" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="dec" text="." GridPane.columnIndex="2" GridPane.rowIndex="4" onAction="#calcInputCallback" styleClass="numbers" />
        <Button fx:id="plus" text="+" GridPane.columnIndex="3" GridPane.rowIndex="3" onAction="#calcInputCallback" styleClass="opperands" />
        <Button fx:id="minus" text="–" GridPane.columnIndex="3" GridPane.rowIndex="2" onAction="#calcInputCallback" styleClass="opperands" />
        <Button fx:id="mult" text="*" GridPane.columnIndex="3" GridPane.rowIndex="1" onAction="#calcInputCallback" styleClass="opperands" />
        <Button fx:id="div" text="/" GridPane.columnIndex="3" GridPane.rowIndex="0" onAction="#calcInputCallback" styleClass="opperands" />
        <Button fx:id="equal" text="=" GridPane.columnIndex="3" GridPane.rowIndex="4" onAction="#calcInputCallback" styleClass="opperands" />
        <Button fx:id="openParenth" text="(" GridPane.columnIndex="1" GridPane.rowIndex="0" onAction="#calcInputCallback" styleClass="numbers"/>
        <Button fx:id="closeParenth" text=")" GridPane.columnIndex="2" GridPane.rowIndex="0" onAction="#calcInputCallback" styleClass="numbers"/>
        <Button fx:id="neg" text="-" GridPane.columnIndex="0" GridPane.rowIndex="4" onAction="#calcInputCallback" styleClass="numbers"/>
        <Button fx:id="space" text="⎵" GridPane.rowIndex="0" GridPane.columnIndex="0" onAction="#calcInputCallback" styleClass="numbers"/>
    </GridPane>
    <Button text="test button" style="-fx-background-color: #FD8D0E"/>
</AnchorPane>


